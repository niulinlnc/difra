<odoo>

    <!-- New sequence for transfers between distributors and theirs final customer -->
    <record model="ir.sequence" id="dist_to_customer">
        <field name="name">dist_to_customer</field>
        <field name="implementation">standard</field>
        <field name="prefix">DIST%(y)s_</field>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
        <field name="number_next_actual">9</field>
    </record>

    <!-- New operation type that contains the transfers between ditributors and theirs customers -->
    <record model="stock.picking.type" id="distributor_to_final_customer">
        <field name="name">Distributor to Customer</field>
        <field name="code">outgoing</field>
        <field name="sequence_id" ref="sale_partner_distributor.dist_to_customer"></field>
    </record>

</odoo>