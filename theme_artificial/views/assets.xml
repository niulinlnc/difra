<?xml version="1.0" encoding="utf-8" ?>
<odoo>
		<!-- LESS ========================================================================== -->


		<template id="demo_frontend_style" name="Theme Shop style"
			inherit_id="website.assets_frontend" active="True" priority="1">
			<xpath expr="." position="inside">
			
				
				<link
					href="/theme_artificial/static/less/options/colors/artificial_main_colors.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/options/colors/customize_modal.less"
					type="text/less" rel="stylesheet" />
			
				<link
					href="/theme_artificial/static/less/carousel_banner.less"
					type="text/less" rel="stylesheet" />
					
				 <link
					href="/theme_artificial/static/less/animate.less"
					type="text/less" rel="stylesheet" /> 			
				<link
					href="/theme_artificial/static/less/s_carousel_imge.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/s_product_carousel.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/custom_owl.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/s_product_purchase.less"
					type="text/less" rel="stylesheet" />
				
				<link
					href="/theme_artificial/static/less/s_tabbed_carousel.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/s_testimonials.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/header_footer.less"
					type="text/less" rel="stylesheet" />
				
				<link
					href="/theme_artificial/static/less/general.less"
					type="text/less" rel="stylesheet" />
				
				<link
					href="/theme_artificial/static/less/s_accordion.less"
					type="text/less" rel="stylesheet" />
					
				 <link
					href="/theme_artificial/static/less/s_product.less"
					type="text/less" rel="stylesheet" /> 
				
				<link
					href="/theme_artificial/static/less/s_product_tab.less"
					type="text/less" rel="stylesheet" />
				
				<link
					href="/theme_artificial/static/less/s_product_detail.less"
					type="text/less" rel="stylesheet" />
				
				<link
					href="/theme_artificial/static/less/padding.less"
					type="text/less" rel="stylesheet" />
					
				<link
					href="/theme_artificial/static/less/navbar-megamenu.less"
					type="text/less" rel="stylesheet" />
					
					
				<script type="text/javascript"
					src="/theme_artificial/static/js/jquery.nicescroll.min.js" />
				
				<script type="text/javascript"
					src="/theme_artificial/static/js/navbarfix.js" />
					
					
					
					

					

			</xpath>
		</template>

		<template id="place_into_bar" inherit_id="website.snippets"
			name="Place into bar">
			<xpath expr="//div[@id='snippet_structure']" position="inside">
				<div class="o_panel_body">
					<t t-snippet="theme_artificial.snippet_carousel_image" t-thumbnail="/theme_artificial/static/image/s-icon.png" />
					<t t-snippet="theme_artificial.snippet_home_bannner" t-thumbnail="/theme_artificial/static/image/s-icon.png" />
					<t t-snippet="theme_artificial.snippet_purchase_progress" t-thumbnail="/theme_artificial/static/image/s-icon.png" />
					<t t-snippet="theme_artificial.snippet_two_images" t-thumbnail="/theme_artificial/static/image/s-icon.png" />
				</div>

			</xpath>
		</template>


	<!-- Color Scheme Assets -->

		<template id="colors_artificial" name="option_colors_artificial"
			inherit_id="website.assets_frontend" active="True" customize_show="True">
			<xpath expr="." position="inside">
				<link
					href="/theme_artificial/static/less/options/colors/colors_artificial.less"
					rel="stylesheet" type="text/less" />
			</xpath>
		</template>
</odoo>



