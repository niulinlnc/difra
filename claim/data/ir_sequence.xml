<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record model="ir.sequence" id="claim_name_sequence">
            <field name="name">Claim Name Sequence</field>
            <field name="code">claim</field>
            <field name="prefix">CL</field>
            <field name="padding">4</field>
        </record>

        <record model="ir.sequence" id="claim.repair_out_sequence">
            <field name="name">Claim Repair Sequence OUT</field>
            <field name="prefix">RS%(range_y)s</field>
            <field name="padding">4</field>
            <field name="use_date_range">True</field>
        </record>

        <record model="ir.sequence" id="claim.repair_in_sequence">
            <field name="name">Claim Repair Sequence IN</field>
            <field name="prefix">RE%(range_y)s</field>
            <field name="padding">4</field>
            <field name="use_date_range">True</field>
        </record>
    </data>
</odoo>